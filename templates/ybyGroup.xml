<templateSet group="ybyGroup">
  <template name="***" value="**&#10; * @description: TODO $end$&#10; * @author $user$&#10; * @date $date$ $time$&#10; * @version 1.0&#10; */" description="类上加注释" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="user" expression="" defaultValue="user()" alwaysStopAt="true" />
    <variable name="date" expression="" defaultValue="date()" alwaysStopAt="true" />
    <variable name="time" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="**" value="** &#10; * @Author milian&#10; * @Description //TODO $end$&#10; * @Date $date$ $time$ &#10; * @Param $param$&#10; * @return $return$&#10; * @throw $throw$      &#10; **/" description="为方法加注释" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="methodParameters()" defaultValue="null" alwaysStopAt="true" />
    <variable name="return" expression="methodReturnType()" defaultValue="null" alwaysStopAt="true" />
    <variable name="throw" expression="expectedType()" defaultValue="null" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="/*" value="/**&#10; *&#10; */" description="变量注释" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>